<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.yakovlev.controllers.TablesModeController">
   <children>
      <TableView fx:id="main_table" onMouseClicked="#selectTableRecordForChanging" layoutX="178.0" layoutY="65.0" prefHeight="311.0" prefWidth="583.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <Button layoutX="177.0" layoutY="14.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="170.0" text="Добавить" onAction="#buttonAdd">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="384.0" layoutY="14.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="170.0" text="Изменить" onAction="#buttonUpdate">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="590.0" layoutY="14.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="170.0" text="Удалить" onAction="#buttonDelete">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ListView fx:id="tables_list" onMouseClicked="#selectTableForView" layoutX="14.0" layoutY="66.0" prefHeight="311.0" prefWidth="148.0" />
      <Label layoutX="49.0" layoutY="33.0" text="Таблицы">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
