<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.yakovlev.controllers.filters.TechnicalInspectionController">
    <children>
        <VBox prefHeight="400.0" prefWidth="600.0">
            <children>
                <StackPane prefHeight="50.0" prefWidth="200.0">
                    <children>
                        <Label text="Получить перечень и общее число владельцев машин не прошедших вовремя техосмотр." textAlignment="CENTER" wrapText="true">
                            <font>
                                <Font size="14.0" />
                            </font></Label>
                    </children></StackPane>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="600.0">
                    <children>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                              <Button alignment="CENTER" mnemonicParsing="false" onAction="#filter" text="Ок" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                              <Label fx:id="size" wrapText="true">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
                    </children>
                    <VBox.margin>
                        <Insets />
                    </VBox.margin></HBox>
                <TableView fx:id="mainTable" prefHeight="250.0" prefWidth="600.0">
                    <columns>
                        <TableColumn prefWidth="75.0" text="C1" />
                        <TableColumn prefWidth="75.0" text="C2" />
                    </columns>
                </TableView>
            </children></VBox>
    </children>
</AnchorPane>
